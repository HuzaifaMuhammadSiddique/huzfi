  def cyclic_shift_right(list_name):
    """ This function shifts all the elements of a list to the left by one index.
        The rightmost index or the last index is replaced by the value at the first index """
  
    new_list = [list_name[-1]]

    for index in range(0, list_name[-2]):
      new_list.append(list_name[index]) 
    
    return new_list
